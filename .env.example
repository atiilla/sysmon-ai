# Sysmon AI Configuration
# Copy this file to .env and fill in your API keys

# Groq API Key for AI-powered analysis
# Get your key from: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# VirusTotal API Key for threat intelligence
# Get your key from: https://www.virustotal.com/gui/my-apikey
VIRUSTOTAL_API_KEY=your_virustotal_api_key_here

# AbuseIPDB API Key for IP reputation checking
# Get your key from: https://www.abuseipdb.com/api
ABUSEIPDB_API_KEY=your_abuseipdb_api_key_here

# Web Interface Configuration (optional)
SYSMON_AI_WEB_PORT=8501
SYSMON_AI_WEB_HOST=localhost

# Logging Configuration (optional)
SYSMON_AI_LOG_LEVEL=INFO
